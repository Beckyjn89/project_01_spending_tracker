<p>Here are all your transactions!</p>

<table>
  <tr>
    <th>Shop</th>
    <th>Tag</th>
    <th>Spent</th>
  </tr>
<% for transaction in @transactions %>
<tr><td><%= transaction.show_merchant_name %> </td>
  <td><%= transaction.show_tag_name %></td>
  <td><%= transaction.spend %></td>
  <td><form method="get" action="/spending-tracker/transactions/<%=transaction.id%>">
  <input type="submit" value="View more"/>
  </form></td>
  <td><form method="get" action="/spending-tracker/transactions/<%=transaction.id%>/edit">
  <input type="submit" value="Edit"/>
  </form></td>
  <td>
    <form method="post" action="/spending-tracker/transactions/<%=transaction.id%>/delete">
    <input type="submit" value="Delete"/>
    </form>
  </td>
</tr>
<% end %>
</table>
